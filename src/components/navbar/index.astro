---
import NavLinks from "./nav-links.astro";

interface Props {
  className?: string;
}

const { className } = Astro.props;
---

<nav class:list={["sticky top-0 z-50 w-full backdrop-blur-sm", className]}>
  <div class="mx-auto flex max-w-4xl items-center justify-end px-3 py-1">
    <div class="flex items-center justify-center gap-2">
      <NavLinks />
    </div>
  </div>
</nav>
